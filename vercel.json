{
  "version": 2,
  "env": [
    "MONGO_URI",
    "EMAIL_PENGIRIM",
    "PASSWORD_EMAIL",
    "NODE_ENV"
  ],
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
